        <TextField fx:id="id" layoutX="213.0" layoutY="166.0" />
        <Button contentDisplay="CENTER" layoutX="461.0" layoutY="166.0" mnemonicParsing="false" onAction="#switchToBookOrOrder" text="Enter" />
      <Label layoutX="272.0" layoutY="230.0" text="Or" />
      <Button fx:id="newCustomer" layoutX="229.0" layoutY="281.0" mnemonicParsing="false" onAction="#switchToAddCustomer" text="Add new Customer" />
      <Button fx:id="backMain" layoutX="38.0" layoutY="361.0" mnemonicParsing="false" onAction="#backToMain" text="Back to Main" />
      <Label fx:id="feedback" layoutX="175.0" layoutY="200.0" prefHeight="17.0" prefWidth="208.0" />
    </children>
